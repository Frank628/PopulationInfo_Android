<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >
    <include 
        android:id="@+id/navgation_top"
        layout="@layout/navigation_layout"
        />
    <RelativeLayout 
		        android:id="@+id/rl_shequ"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_leftshequ" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="社区"
		            />
		        <TextView
		            android:id="@+id/tv_shequ"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftshequ"
		            android:singleLine="true"
		            android:drawableRight="@drawable/ico_search_next"
		            android:drawablePadding="5dp"
		            android:background="@color/transaction"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <RelativeLayout 
		        android:id="@+id/rl_bianhao"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_leftbianhao" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="房屋编号"
		            />
		        <com.jinchao.population.widget.ValidateEidtText
		            android:id="@+id/edt_bianhao"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftbianhao"
		            android:singleLine="true"
		            android:hint="请输入房屋编号"
					android:maxLength="6"
		            android:textColorHint="@color/title4"
		            android:background="@color/transaction"
		            android:inputType="text"
					android:drawablePadding="5dp"
					app:vd_textErrorColor="@color/red"
					app:vd_textRightColor="@color/title2"
					app:vd_drawable_right_right="@drawable/icon_gou"
					app:vd_drawable_right_error="@drawable/icon_gantan"
					app:vd_validate_type="house_code"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <RelativeLayout 
		        android:id="@+id/rl_dizhibianji"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
				android:minHeight="@dimen/relative_row_height"
		        >
		        <TextView
		            android:id="@+id/tv_leftdizhibianji" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="房屋地址编辑"
		            />
		        <TextView
		            android:id="@+id/tv_bianji"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftdizhibianji"
		            android:drawableRight="@drawable/ico_search_next"
		            android:drawablePadding="5dp"
		            android:text="点击编辑房屋地址"
		            android:background="@color/transaction"
					android:layout_centerVertical="true"
		            android:layout_marginLeft="10dp"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <RelativeLayout 
		        android:id="@+id/rl_fangdongxingming"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_fangdongxingming" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="房东姓名"
		            />
		        <EditText
		            android:id="@+id/edt_fangdongxingming"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_fangdongxingming"
		            android:singleLine="true"
		            android:hint="请输入房东姓名"
		            android:textColorHint="@color/title4"
		            android:background="@color/transaction"
					android:maxLength="30"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_shenf" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="身份证号"
		            />
		        <com.jinchao.population.widget.ValidateEidtText
		            android:id="@+id/edt_shenfenzheng"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_shenf"
		            android:singleLine="true"
		            android:hint="请输入房东身份证号"
		            android:textColorHint="@color/title4"
		            android:background="@color/transaction"
					android:maxLength="18"
					app:vd_textErrorColor="@color/red"
					app:vd_textRightColor="@color/title2"
					app:vd_drawable_right_right="@drawable/icon_gou"
					app:vd_drawable_right_error="@drawable/icon_gantan"
					app:vd_validate_type="id_card"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <RelativeLayout 
		        android:id="@+id/rl_lianxi"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_lianxidianhua" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="联系方式"
		            />
		        <com.jinchao.population.widget.ValidateEidtText
		            android:id="@+id/edt_dianhua"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_lianxidianhua"
		            android:singleLine="true"
		            android:hint="请输入联系方式"
		            android:textColorHint="@color/title4"
		            android:background="@color/transaction"
		            android:inputType="number"
					app:vd_textErrorColor="@color/red"
					app:vd_textRightColor="@color/title2"
					app:vd_drawable_right_right="@drawable/icon_gou"
					app:vd_drawable_right_error="@drawable/icon_gantan"
					app:vd_validate_type="mobile"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="1dp"
		        android:background="#CACACA"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
</LinearLayout>

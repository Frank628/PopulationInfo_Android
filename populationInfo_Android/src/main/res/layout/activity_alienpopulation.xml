<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="#f5f5f5"
    android:orientation="vertical" >
    <include 
        android:id="@+id/navgation_top"
        layout="@layout/navigation_layout"

        />
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        >
        <ImageView 
            android:id="@+id/iv_register"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_register"
            />
        
        <ImageView 
            android:id="@+id/iv_personinfocheck"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_personinfocheck"
            android:layout_marginLeft="50dp"
            />
    </LinearLayout>
	<LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        >
        <ImageView 
            android:id="@+id/iv_rentalhouseregist"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_rentalhouseregist"
            />
        
        <ImageView 
            android:id="@+id/iv_rentalhousecheck"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_rentalhousecheck"
            android:layout_marginLeft="50dp"
            />
    </LinearLayout>
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        >
        <ImageView 
            android:id="@+id/iv_photo"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_photo"
            />
        
        <ImageView 
            android:id="@+id/iv_sys"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/btn_alien_sys"
            android:layout_marginLeft="50dp"
            />
    </LinearLayout>
</LinearLayout>

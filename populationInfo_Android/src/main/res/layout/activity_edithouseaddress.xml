<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >
    <include 
        android:id="@+id/navgation_top"
        layout="@layout/navigation_layout"
        />
    <RelativeLayout 
		        android:id="@+id/rl_jieluxiang"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:background="@drawable/white_click_bg2"
		        android:paddingLeft="@dimen/layout_leftright_padding"
		        android:paddingRight="@dimen/layout_leftright_padding"
		        >
		        <TextView
		            android:id="@+id/tv_leftjieluxiang" 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="@dimen/small_size"
		            android:textColor="@color/title1"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="街路巷"
		            />
		        <EditText
		            android:id="@+id/edt_jieluxiang"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center_vertical|right"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftjieluxiang"
					android:layout_marginLeft="5dp"
		            android:singleLine="true"
		            android:hint="查找并选择街路巷"
		            android:textColorHint="@color/title3"
		            android:background="@null"
		            android:editable="false"
		            />
		    </RelativeLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="15dp"
		        android:background="#f5f5f5"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:orientation="horizontal"
		        android:background="@drawable/improtant_people_bg"
				android:layout_gravity="center_vertical"
				android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp"
		        >
		        <RelativeLayout 
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:layout_weight="2"
		        >
		        <TextView
		            android:id="@+id/tv_leftmenpaihao" 
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:textSize="@dimen/small_size"
		            android:textColor="@color/white"
					android:background="@drawable/improtant_people_tv_bg"
		            android:gravity="center_vertical"
					android:paddingLeft="10dp"
					android:paddingRight="10dp"
		            android:layout_centerVertical="true"
		            android:text="门牌号"
		            />
		        <EditText
		            android:id="@+id/edt_menpaihao"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftmenpaihao"
					android:background="@null"
		            android:singleLine="true"
		            android:hint="输入门牌号"
					android:inputType="number"
		            android:textColorHint="@color/title3"
					android:maxLength="4"
		            android:layout_marginLeft="15dp"
		            />
		    </RelativeLayout>
				<View
					android:layout_width="1dp"
					android:layout_height="match_parent"
					android:background="#616161"/>
		    <EditText
		        android:id="@+id/edt_menpaihaodanwei"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:textSize="@dimen/micro_middle_size"
		        android:textColor="@color/title2"
		        android:gravity="left|center_vertical"
		        android:hint="请选择"
				android:textColorHint="@color/title3"
				android:paddingLeft="10dp"
				android:paddingRight="5dp"
		        android:drawableRight="@drawable/icon_down"
				android:background="@null"
				android:editable="false"
				android:focusable="false"
		        />
		    </LinearLayout>
			<View 
		        android:layout_width="match_parent"
		        android:layout_height="15dp"
		        android:background="#f5f5f5"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:orientation="horizontal"
				android:background="@drawable/improtant_people_bg"
				android:layout_gravity="center_vertical"
				android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp"
		        >
		        <RelativeLayout 
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:layout_weight="2"
		        >
		        <TextView
		            android:id="@+id/tv_leftfuhao" 
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:textSize="@dimen/small_size"
		            android:textColor="@color/white"
		            android:gravity="center_vertical"
					android:background="@drawable/improtant_people_tv_bg"
		            android:layout_centerVertical="true"
		            android:text="副    号"
					android:paddingLeft="10dp"
					android:paddingRight="10dp"
		            />
		        <EditText
		            android:id="@+id/edt_fuhao"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="center"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:layout_toRightOf="@+id/tv_leftfuhao"
		            android:singleLine="true"
		            android:hint="输入副号"
		            android:textColorHint="@color/title3"
					android:inputType="number"
					android:maxLength="2"
					android:background="@null"
		            android:layout_marginLeft="15dp"
		            />
		    </RelativeLayout>
				<View
					android:layout_width="1dp"
					android:layout_height="match_parent"
					android:background="#616161"/>
		    <EditText
		        android:id="@+id/edt_fuhaodanwei"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:textSize="@dimen/micro_middle_size"
		        android:textColor="@color/title2"
		        android:gravity="left|center_vertical"
		        android:hint="请选择"
				android:paddingTop="7dp"
				android:paddingBottom="7dp"
		        android:drawableRight="@drawable/icon_down"
				android:textColorHint="@color/title3"
				android:paddingLeft="10dp"
				android:paddingRight="5dp"
				android:background="@null"
				android:editable="false"
				android:focusable="false"
		        />
		    </LinearLayout>
		    <View 
		        android:layout_width="match_parent"
		        android:layout_height="15dp"
		        android:background="#f5f5f5"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		    <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/relative_row_height"
		        android:orientation="horizontal"
		        android:background="@drawable/improtant_people_bg"
				android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp"
				android:layout_gravity="center_vertical"
		        >
		    	<TextView
		            android:id="@+id/tv_leftlouzhuanghao"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:textSize="@dimen/small_size"
					android:textColor="@color/white"
					android:gravity="center_vertical"
					android:background="@drawable/improtant_people_tv_bg"
		            android:text="楼幢号"
					android:paddingLeft="10dp"
					android:paddingRight="10dp"
		            />
		        <LinearLayout 
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/relative_row_height"
			        android:orientation="horizontal"
			        android:layout_marginLeft="15dp"
					android:layout_gravity="center_vertical"
		        	>
		        <EditText
			        android:id="@+id/edt_lou"
			        android:layout_width="0dp"
			        android:layout_height="wrap_content"
			        android:textSize="@dimen/micro_middle_size"
			        android:layout_weight="1"
			        android:textColor="@color/title2"
			        android:layout_toRightOf="@+id/tv_leftlouzhuanghao"
			        android:gravity="center"
			        android:hint="请选择"
			        android:drawableRight="@drawable/icon_down"
			        android:background="@null"
					android:textColorHint="@color/title3"
					android:editable="false"
					android:focusable="false"
		        />
					<View
						android:layout_width="1dp"
						android:layout_height="match_parent"
						android:background="#616161"/>
		    <EditText
		            android:id="@+id/edt_louhao"
		            android:layout_width="0dp"
		            android:layout_height="match_parent"
		            android:layout_weight="1"
		            android:gravity="center"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:singleLine="true"
		            android:textColorHint="@color/title3"
		            android:hint="输入楼号"
					android:inputType="number"
					android:maxLength="3"
					android:background="@null"
		            />
			<View
					android:layout_width="1dp"
					android:layout_height="match_parent"
					android:background="#616161"/>
		    <EditText
		        android:id="@+id/edt_zhuanghao"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:textSize="@dimen/micro_middle_size"
		        android:textColor="@color/title2"
		        android:gravity="center"
		        android:hint="请选择"
				android:textColorHint="@color/title3"
		        android:drawableRight="@drawable/icon_down"
		        android:background="@null"
				android:editable="false"
				android:focusable="false"
		        />
		    </LinearLayout>
		    </LinearLayout>
		     <View 
		        android:layout_width="match_parent"
		        android:layout_height="15dp"
		        android:background="#f5f5f5"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		     <LinearLayout 
		         android:layout_width="match_parent"
		         android:layout_height="@dimen/relative_row_height"
		         android:orientation="horizontal"
				 android:background="@drawable/improtant_people_bg"
				 android:layout_marginLeft="5dp"
				 android:layout_marginRight="5dp"
				 android:layout_gravity="center_vertical"
		         >
		         <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:textSize="@dimen/small_size"
		            android:textColor="@color/white"
		            android:gravity="center_vertical"
		            android:text="单    元"
					 android:background="@drawable/improtant_people_tv_bg"
					 android:paddingLeft="10dp"
					 android:paddingRight="10dp"
		            />
		         <EditText
			        android:id="@+id/edt_danyuan"
			        android:layout_width="0dp"
			        android:layout_height="wrap_content"
			        android:layout_weight="1"
			        android:textSize="@dimen/micro_middle_size"
			        android:textColor="@color/title2"
			        android:gravity="left|center_vertical"
			        android:hint="请选择"
					 android:textColorHint="@color/title3"
			        android:paddingLeft="5dp"
					 android:paddingRight="5dp"
			        android:drawableRight="@drawable/icon_down"
			        android:background="@null"
			        android:layout_marginLeft="15dp"
					 android:editable="false"
					 android:focusable="false"
		        />
		     </LinearLayout>
		     <View 
		        android:layout_width="match_parent"
		        android:layout_height="15dp"
		        android:background="#f5f5f5"
		        android:layout_marginLeft="@dimen/layout_leftright_padding"
		        android:layout_marginRight="@dimen/layout_leftright_padding"
		        />
		     <LinearLayout 
		         android:layout_width="match_parent"
		         android:layout_height="@dimen/relative_row_height"
		         android:orientation="horizontal"
				 android:background="@drawable/improtant_people_bg"
				 android:layout_marginLeft="5dp"
				 android:layout_marginRight="5dp"
				 android:layout_gravity="center_vertical"
		         >
		         <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:textSize="@dimen/small_size"
		            android:textColor="@color/white"
		            android:gravity="center_vertical"
		            android:layout_centerVertical="true"
		            android:text="室    号:"
					 android:background="@drawable/improtant_people_tv_bg"
					 android:paddingLeft="12dp"
					 android:paddingRight="12dp"
		            />
		         <LinearLayout 
		             android:layout_width="match_parent"
			         android:layout_height="@dimen/relative_row_height"
			         android:orientation="horizontal"
					 android:layout_gravity="center_vertical"
		             >
		        <EditText
		            android:id="@+id/edt_shihao"
		            android:layout_width="0dp"
		            android:layout_height="match_parent"
		            android:layout_weight="1"
		            android:gravity="center"
		            android:textSize="@dimen/micro_middle_size"
		            android:textColor="@color/title2"
		            android:singleLine="true"
		            android:textColorHint="@color/title3"
		            android:layout_marginLeft="15dp"
		            android:hint="输入室号"
					android:maxLength="4"
					android:inputType="number"
					android:background="@null"
		            />
					 <View
						 android:layout_width="1dp"
						 android:layout_height="match_parent"
						 android:background="#616161"/>
		         <EditText
			        android:id="@+id/edt_shihaodanwei"
			        android:layout_width="0dp"
			        android:layout_height="wrap_content"
			        android:layout_weight="1"
			        android:textSize="@dimen/micro_middle_size"
			        android:textColor="@color/title2"
			        android:gravity="left|center_vertical"
			        android:hint="请选择"
					 android:textColorHint="@color/title3"
					android:paddingLeft="5dp"
					 android:paddingRight="5dp"
					 android:paddingTop="7dp"
					 android:paddingBottom="7dp"
			        android:drawableRight="@drawable/icon_down"
			        android:background="@null"
					 android:editable="false"
					 android:focusable="false"
		       		 />
		         </LinearLayout>
		     </LinearLayout>
</LinearLayout>
